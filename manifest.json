{
  "name": "Chrome History",
  "version": "1.0",
  "manifest_version": 1,
  "description": "Visualized Chrome History",
  "background": { 
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Chrome History"
  },
  "conent_scripts": [
    { "js": ["assets/scripts/chronoline/raphael-min.js"] }
  ],
  "permissions": [
    "tabs", "history", "browsingData", "bookmarks", "chrome://favicon/*"
  ]
}
